{
    "name": "@joaoleite/n8n-nodes-toqan",
    "version": "0.1.0",
    "description": "n8n Community Node for Toqan AI - AI-powered conversations with automatic response polling and multiple outputs",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "toqan",
        "ai",
        "conversation",
        "chatbot",
        "polling",
        "async",
        "automation"
    ],
    "license": "MIT",
    "homepage": "https://github.com/joaoleite/n8n-nodes-toqan",
    "author": {
        "name": "Jo√£o Leite",
        "email": "joao@example.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/joaoleite/n8n-nodes-toqan.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "test": "node --loader ts-node/esm tests/run_tests.mjs",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials package.json",
        "lintfix": "eslint nodes credentials package.json --fix"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/ToqanApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Toqan/Toqan.node.js",
            "dist/nodes/Toqan/ToqanAgent.node.js",
            "dist/nodes/Toqan/ToqanChatModel.node.js"
        ]
    },
    "devDependencies": {
        "@types/node": "^18.16.0",
        "@typescript-eslint/parser": "^5.59.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.40.0",
        "eslint-plugin-n8n-nodes-base": "^1.11.0",
        "gulp": "^4.0.2",
        "n8n-workflow": "^1.0.0",
        "prettier": "^2.8.8",
        "typescript": "^5.0.4"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "dependencies": {
        "form-data": "^4.0.0",
        "@langchain/core": "^0.3.0"
    }
}